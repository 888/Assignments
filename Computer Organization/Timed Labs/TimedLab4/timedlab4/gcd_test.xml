<?xml version="1.0"?>
<test-suite>

    <test-case>
        <name>gcd(47,47)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-subr>
                <name>GCD</name>
                <stack>xF000</stack>
                <r7>x5000</r7>
                <r5>xCAFE</r5>
                <params>47,47</params>
            </test-subr>
        </input>
        <output>
            <test-subr>
                <answer>47</answer>
                <points>
                    <answer>3</answer>
                    <params>4</params>
                    <r7>3</r7>
                    <r6>3</r6>
                    <r5>3</r5>
                </points>
                <deductions-per-mistake>1</deductions-per-mistake>
            </test-subr>
        </output>
    </test-case>

