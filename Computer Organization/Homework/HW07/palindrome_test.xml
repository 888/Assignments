<test-suite>
	<test-case> 
		<name>length('RAceCaR') == 7</name>
		<has-max-executions>1</has-max-executions>
		<max-executions>1000000</max-executions>
		<randomize>1</randomize>
		<input>
			<test-subr><name>LENGTH</name><stack>xF000</stack><r7>x5000</r7><r5>xCAFE</r5><params>x5002</params></test-subr>
			<test-value><address>x5001</address><value>x5002</value></test-value>
			<test-string><address>x5001</address><value>RAceCaR</value></test-string>
		</input>
		<output>
			<test-subr>
				<answer>7</answer>
				<locals>82</locals>
				<points>
					<answer>2</answer>
					<params>2</params>
					<r7>2</r7>
					<r6>2</r6>
					<r5>2</r5>
					<locals>2</locals>
				</points>
				<deductions-per-mistake>2</deductions-per-mistake>
			</test-subr>
		</output>
	</test-case>
