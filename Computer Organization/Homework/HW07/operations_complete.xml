    <test-case>
        <name>x3000 Test 1 (XOR(1,2) == 3)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value>
                <address>A</address>
                <value>1</value>
            </test-value>
            <test-value>
                <address>B</address>
                <value>2</value>
            </test-value>
            <test-value>
                <address>OP</address>
                <value>0</value>
            </test-value>
        </input>
        <output>
            <test-value>
                <address>ANSWER</address>
                <value>3</value>
            </test-value>
        </output>
    </test-case>
